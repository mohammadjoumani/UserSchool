<?xml version="1.0" encoding="utf-8"?><RelativeLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/colorGray"    tools:context=".view.fragment.AbseceStudentFragment">    <ProgressBar        android:id="@+id/progressBarAbseceStudentFragment"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerInParent="true"        android:visibility="gone"/>    <ImageView        android:id="@+id/imgAbseceStudentFragmentEmpty"        android:layout_width="220dp"        android:layout_height="220dp"        android:layout_centerInParent="true"        android:src="@drawable/ic_empty"        android:visibility="gone" />    <RelativeLayout        android:id="@+id/toolBarFragmentHome"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_margin="15dp">        <TextView            android:id="@+id/txtFragmentHomeNameFragment"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:fontFamily="@font/roboto_medium"            android:text="Absece "            android:textColor="@color/colorBlack"            android:textSize="26sp" />        <ImageView            android:layout_width="35dp"            android:layout_height="35dp"            android:src="@drawable/ic_account_focusedd"            android:layout_alignParentEnd="true"            android:layout_centerVertical="true"/>    </RelativeLayout>    <TableLayout        android:id="@+id/tableLayoutAbseceFragment"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical"        android:layout_below="@id/toolBarFragmentHome"        android:visibility="visible">        <TableRow            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="horizontal"            android:padding="10dp"            android:layout_marginStart="10dp"            android:layout_marginEnd="10dp"            android:background="@drawable/bg_button_sin_in">            <TextView                android:id="@+id/txtTableLayoutSubject"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_weight="0.25"                android:fontFamily="@font/roboto_bold"                android:gravity="center"                android:text="Subject"                android:textColor="@color/colorWhite"                android:textSize="16sp" />            <TextView                android:id="@+id/txtTableLayoutFound"                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="0.25"                android:fontFamily="@font/roboto_bold"                android:gravity="center"                android:text="Found"                android:textColor="@color/colorWhite"                android:textSize="16sp" />            <TextView                android:id="@+id/txtTableLayoutDate"                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="0.25"                android:fontFamily="@font/roboto_bold"                android:gravity="center"                android:text="Date"                android:textColor="@color/colorWhite"                android:textSize="16sp" />        </TableRow>    </TableLayout>    <androidx.recyclerview.widget.RecyclerView        android:id="@+id/recyclerFragmentAbseceStudent"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_below="@+id/tableLayoutAbseceFragment"        android:layout_alignParentBottom="true"        android:layout_centerHorizontal="true"        android:layout_marginStart="10dp"        android:layout_marginTop="4dp"        android:layout_marginEnd="5dp"        android:layout_marginBottom="0dp"        android:orientation="vertical"        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"        tools:listitem="@layout/item_table_abcese" />    <com.google.android.material.floatingactionbutton.FloatingActionButton        android:id="@+id/floatingActionFragmentAbseceByDate"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_alignParentBottom="true"        android:layout_marginEnd="15dp"        android:layout_marginBottom="5dp"        android:clickable="true"        android:elevation="8dp"        app:backgroundTint="@color/colorPurple"        app:srcCompat="@drawable/ic_date"        android:visibility="visible"/></RelativeLayout>